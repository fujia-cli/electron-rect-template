{
  "name": "<%= packageName %>",
  "version": "<%= version %>",
  "description": "",
  "main": "./app/dist/main.js",
  "scripts": {
    "start:main": "webpack --config ./app/build/webpack.main.dev.js && electron ./app/dist/main.js",
    "start:render": "webpack-dev-server --config ./app/build/webpack.render.dev.js",
    "start": "webpack-dev-server --config ./app/build/webpack.web.dev.js",
    "build:main": "webpack --config ./app/build/webpack.main.prod.js",
    "build:render": "webpack --config ./app/build/webpack.render.prod.js",
    "build:dll": "webpack --config ./app/build/webpack.dll.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "npm run clean && npm run build:main && npm run build:render && npm run dist",
    "install:electron": "ELECTRON_MIRROR=https://cdn.npm.taobao.org/dist/electron/ npm install electron",
    "clean": "rimraf app/dist package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "<%= packageName %>.fujia.site",
    "productName": "<%= packageName %>",
    "copyright": "Copyright Â© 2023 ${author}",
    "extends": null,
    "files": [
      "app/dist/**/*",
      "package.json",
      "node_modules/"
    ],
    "directories": {
      "output": "package"
    },
    "mac": {
      "icon": "assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 410,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "icon": "assets/icons/",
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": "deb",
      "category": "Office",
      "icon": "assets/icons/"
    }
  },
  "keywords": [
    "electron"
  ],
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@ui-puzzles/rect": "^0.1.3",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "html-webpack-tags-plugin": "^3.0.1",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1"
  }
}
